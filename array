#include <stdio.h>
#include <stdlib.h>
#include <time.h>

#define SIZE 10

int main() {
    int arr[SIZE];
    
    // Seed the random number generator
    srand(time(NULL));
    
    // Populate the array with random numbers
    for (int i = 0; i < SIZE; i++) {
        arr[i] = rand() % 100; // Random number between 0 and 99
    }
    
    // Print the array
    printf("Random array:\n");
    for (int i = 0; i < SIZE; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
